# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Database connections
# Connect to Supabase via connection pooling
DATABASE_URL="postgresql://postgres.your-project:your-password@aws-1-eu-west-1.pooler.supabase.com:6543/postgres?pgbouncer=true"
DB_URL=postgresql://postgres:your-password@db.your-project.supabase.co:5432/postgres

# JWT configuration
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=1d

# External API keys
MOCKAROO_KEY=your-mockaroo-key
TWELVEDATA_KEY=your-twelvedata-key
LOGO_DEV_SECRET_KEY=your-logo-dev-secret-key
LOGO_DEV_PUBLIC_KEY=your-logo-dev-public-key

# Yahoo Finance logging
YF_LOG_ERRORS=true
YF_LOG_OPTIONS_ERRORS=true
